<h1>Listing mibs</h1>

<table>
  <thead>
    <tr>
      <th><%= Mib.human_attribute_name(:file_name_mib) %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mibs.each do |mib| %>
      <tr>
        <td><%= mib.file_name_mib %></td>
        <td><%= link_to 'Show', mib %></td>
        <td><%= link_to 'Edit', edit_mib_path(mib) %></td>
        <td><%= link_to 'Destroy', mib, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mib', new_mib_path %>
